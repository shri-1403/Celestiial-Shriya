{
	"name": "Celestial Shriya",
	"colors": {
		"editor.background": "#dbff79",
		"editor.foreground": "#4d2d05",
		"editor.lineHighlightBackground": "#072e4e22",
        "editorCursor.foreground": "#000435", 
	},
	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#2F57FF"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#194d01"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#9c103a"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#2e1c04"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#f10761"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#053257"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#05125a"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#1286c5"
			}
		},
		{
			"scope": [
				"source.css entity.other.attribute-name",
				"source.css.less entity.other.attribute-name.id",
				"source.scss entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#3d310c"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#3d2222"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#0e3b8a"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#5f13da"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#461807"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#1587e4"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#4e3500"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#4d1919"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#0069be"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#301105"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#2e4b1f"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#2f4f61"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#2c343b"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#ca2d2d"
			}
		},
		{
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#021627"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#256807"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#530e94"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#492110"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#332828"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js"
			],
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less"
			],
			"settings": {
				"foreground": "#830808"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#0872c9"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#062036"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#700c0c"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#0a79d4"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#153305"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#2e0169"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#214e09"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#2b1111"
			}
		},
		{
			"name": "coloring of the TS this",
			"scope": "variable.language.this",
			"settings": {
				"foreground": "#0a4270"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#1f1f09"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",
				"source.cs storage.type"
			],
			"settings": {
				"foreground": "#580830"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#0b5e4d"
			}
		},
		{
			"name": "Control flow keywords",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#a02996"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#0a5e3b"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#094566"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#3d1302"
			}
		}
	]
}
